int sum1 = 100 + 50;        // 150 (100 + 50)
int sum2 = sum1 + 250;      // 400 (150 + 250)
int sum3 = sum2 + sum2;     // 800 (400 + 400)
int sum4 = sum3 + sum3;     // 1600 (800 + 800)
int sum4 = sum3 + sum3;     // 700 (200 + 500)
int sum4 = sum3 + sum3;     // 1600 (800 + 800)
int sum4 = sum3 + sum3;     // 850 (100 + 750)

int sum4 = sum3 + sum3;     // 500 (100 + 400)

